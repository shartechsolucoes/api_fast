{
	"name": "apifastmanager",
	"version": "1.0.0",
	"description": "",
	"license": "ISC",
	"author": "",
	"type": "commonjs",
	"main": "app.js",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"start": "ts-node src/index.ts",
		"dev": "nodemon --watch src --ext ts --exec ts-node ./app.ts",
		"test": "echo \"Error: no test specified\" && exit 1",
		"typeorm": "typeorm-ts-node-commonjs",
		"sync:db": "npx typeorm schema:sync --config ./src/data-source.ts",
		"migration:run": "npx typeorm-ts-node-commonjs migration:run --config ./src/data-source.ts",
		"migration:generate": "npx typeorm-ts-node-commonjs migration:generate --config ./src/data-source.ts -n",
		"migration:revert": "npx typeorm-ts-node-commonjs migration:revert --config ./src/data-source.ts"
	},
	"devDependencies": {
		"@types/node": "^24.1.0",
		"nodemon": "^3.1.10",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@fastify/mysql": "^5.0.2",
		"bcryptjs": "^3.0.2",
		"fastify": "^5.4.0",
		"fastify-jwt": "^4.1.3",
		"fastify-plugin": "^5.0.1",
		"mysql": "^2.18.1",
		"pg": "^8.14.1",
		"reflect-metadata": "^0.2.2",
		"typeorm": "0.3.25"
	}
}
